<%if @current_user != nil%>
  <h1><%= @current_user["username"] %></h1>
<h1>New Place</h1>

<form action="/places" method="post">
  <p>
    <label for="name_input">Name</label>
    <input type="text" name="name" id="name_input">
  </p>

  <button>Save</button>
</form>
<% else %>
   
    <p>You need to be logged in to see the content</p>
  
  <%end%>
